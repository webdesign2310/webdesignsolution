.cd-timeline-block:after { content: ""; display: table; clear: both; }
.cd-timeline-block:first-child { margin-top: -50px; }
.cd-timeline-block:last-child { margin-bottom: 0; margin-top: 35px }
.cd-timeline-img { position: absolute; top: 0; left: 0; width: 40px; height: 40px; border-radius: 50%; box-shadow: 0 0 0 5px #6a136f; z-index: 99; }
