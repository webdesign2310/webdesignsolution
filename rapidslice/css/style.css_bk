.cd-timeline-content p { line-height: 20px; font-size: 16px; font-family: 'effraregular'; text-align: left; margin: 0px; padding: 10px 0; }
.cd-timeline-content::before { content: ''; position: absolute; top: 16px; right: 100%; height: 6px; width: 100%; background: #6a136f; z-index: 10; }
